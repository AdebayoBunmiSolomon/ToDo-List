{
  "expo": {
    "plugins": [
      [
        "expo-notifications",
        {
          "sounds": ["./assets/notify_sound.wav"]
        }
      ]
    ],
    "name": "MeTodo",
    "slug": "MeTodo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon1.png",
    "splash": {
      "image": "./assets/splash1.png",
      "resizeMode": "cover",
      "backgroundColor": "#F4C400"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon1.png",
        "backgroundColor": "#F4C400"
      },
      "package": "com.anonymous.todolist_main"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
